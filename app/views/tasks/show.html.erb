<h2><%= @task.title %></h2>
<h4><%= @task.details %></h4>
<h4><%= @task.completed %></h4>

<h6><%= link_to 'Back to list', task_path(@task) %></h6>
<h6><%= link_to 'Edit', edit_task_path(@task) %></h6>
<h6><%= link_to 'Delete', task_path(@task), method: :delete, data: {confirm: "Are you sure?"} %></h6>
